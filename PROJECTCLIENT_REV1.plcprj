					<iecDeclaration active="FALSE"/>
					<sourceCode type="ST">
						<![CDATA[     
CASE I_selettore OF
  
	0:
    	IF (I_selettore = 0 ) AND (B_ora = TRUE ) THEN
            	SELETTORE_A_0_M := TRUE;
    	ELSE
            	SELETTORE_A_0_M := FALSE;
        END_IF;                
    1:
        IF (I_selettore = 1 ) AND (B_ora = FALSE )THEN
                SELETTORE_A_0_M := TRUE;
        ELSE
                SELETTORE_A_0_M:= FALSE;
        END_IF;
    2:
        IF (I_selettore = 2 ) AND (B_ora = TRUE )THEN
                SELETTORE_A_0_M := TRUE;
        ELSE
                SELETTORE_A_0_M := FALSE;
        END_IF;
        
END_CASE;

]]>
